{
  "name": "media-wizard",
  "version": "1.0.0",
  "description": "CLI wizard for transcribing and generating tutorials from audio/video files",
  "main": "dist/index.js",
  "bin": {
    "media-wizard": "dist/index.js"
  },
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "prepare": "npm run build",
    "dev": "node scripts/start.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:run": "vitest run"
  },
  "keywords": [
    "cli",
    "wizard",
    "transcribe",
    "tutorial",
    "ink"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.17"
  },
  "files": [
    "dist/**",
    "prompts/**"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/credential-providers": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@google/genai": "^1.22.0",
    "dotenv": "^17.2.3",
    "glob": "^11.0.0",
    "ink": "^4.4.1",
    "ink-text-input": "^5.0.1",
    "lru-cache": "^11.2.2",
    "mime": "^4.1.0",
    "nanoid": "^5.0.9",
    "react": "^18.2.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "aws-sdk-client-mock": "^4.1.0",
    "ink-testing-library": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
